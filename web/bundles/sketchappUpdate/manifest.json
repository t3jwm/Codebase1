{
	"manifest_version": 2,
	"offline_enabled": true,
	"name": "Sketchpad 3.5",
	"short_name": "Sketchpad",
	"version": "3.7.4.24",
	"description": "Draw, Create, Share!",
	"icons": {
		"16": "media/ux/favicon/16.png",
		"48": "media/ux/favicon/48.png",
		"128": "media/ux/favicon/128.png"
	},
	"app": {
		"background": {
			"scripts": [
				"chrome-app/background.js"
			]
		}
	},
	"optional_permissions": [
		"videoCapture"
	],
	"permissions": [
		"https://sketch.io/",
		"https://static.sketch.io/",
		"https://www.googleapis.com/",
		"https://accounts.google.com/",
		"https://www.google-analytics.com/",
		"identity",
		"storage",
		"fileSystem",
		"fileSystem.write",
		"fullscreen",
		"syncFileSystem",
		"unlimitedStorage"
	],
	"oauth2": {
		"client_id": "828302415973-3tq345a2c72koncls5nfllgnrbn9kfa8.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/drive"
		]
	}
}