{
	"manifest_version": 2,
	"offline_enabled": true,
	"name": "Sketchpad 3.5",
	"short_name": "Sketchpad",
	"version": "3.6.7.39",
	"description": "Draw, Create, Share!",
	"icons": {
		"16": "media/ux/favicon/16.png",
		"48": "media/ux/favicon/48.png",
		"128": "media/ux/favicon/128.png"
	},
	"app": {
		"background": {
			"scripts": [
				"chrome/background.js"
			]
		}
	},
	"optional_permissions": [
		"videoCapture"
	],
	"permissions": [
		"https://sketch.io/",
		"https://apis.google.com/",
		"https://www.google-analytics.com/",
		"identity",
		"storage",
		"fileSystem",
		"fileSystem.write",
		"fullscreen",
		"syncFileSystem",
		"unlimitedStorage"
	]
}